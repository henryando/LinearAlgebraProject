Cell["\<\
Given that the area of the unit circle measures out to \[Pi] square units, \
what does the area of the ellipse plotted above measure out to?\
\>", "Text"],

Cell["L.11)", "Subsubsection"],

Cell["Go with  this matrix A:", "Text"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"xstretch", "=", 
   RowBox[{
    FractionBox["1", "2"], " ", 
    RowBox[{"Random", "[", 
     RowBox[{"Integer", ",", 
      RowBox[{"{", 
       RowBox[{"1", ",", "5"}], "}"}]}], "]"}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"ystretch", "=", 
   RowBox[{
    FractionBox["1", "6"], " ", 
    RowBox[{"Random", "[", 
     RowBox[{"Integer", ",", 
      RowBox[{"{", 
       RowBox[{"1", ",", "5"}], "}"}]}], "]"}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"Clear", "[", 
   RowBox[{
   "x", ",", "y", ",", "t", ",", "hitplotter", ",", "hitpointplotter", ",", 
    "pointcolor", ",", "matrix2D"}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"tlow", ",", "thigh"}], "}"}], "=", 
   RowBox[{"{", 
    RowBox[{"0", ",", 
     RowBox[{"2", " ", "\[Pi]"}]}], "}"}]}], ";"}], "\n", 
