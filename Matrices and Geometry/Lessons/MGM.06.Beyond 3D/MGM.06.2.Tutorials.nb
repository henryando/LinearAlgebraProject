Cell[CellGroupData[{

Cell["T.5.a) Going from linear formulas to matrices", "Subsubsection"],

Cell["\<\
Here's a common way of delivering a linear system:
First, you are given a list of linear formulas:\
\>", "Text"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Clear", "[", "x", "]"}], ";"}], " "}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"linearformulas", "=", "\[IndentingNewLine]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"2.1", " ", 
        RowBox[{"x", "[", "1", "]"}]}], "+", 
       RowBox[{"2.3", " ", 
        RowBox[{"x", "[", "2", "]"}]}], "+", 
       RowBox[{"3.4", " ", 
        RowBox[{"x", "[", "3", "]"}]}], "-", 
       RowBox[{"5.4", " ", 
        RowBox[{"x", "[", "4", "]"}]}], "-", 
       RowBox[{"0.5", " ", 
        RowBox[{"x", "[", "5", "]"}]}], "-", 
       RowBox[{"2.5", " ", 
        RowBox[{"x", "[", "6", "]"}]}]}], ",", 
      RowBox[{
       RowBox[{
        RowBox[{"-", "1.5"}], " ", 
        RowBox[{"x", "[", "1", "]"}]}], "-", 
       RowBox[{"0.4", " ", 
        RowBox[{"x", "[", "2", "]"}]}], "+", 
       RowBox[{"13.5", " ", 
        RowBox[{"x", "[", "4", "]"}]}], "-", 
       RowBox[{"0.7", " ", 
        RowBox[{"x", "[", "5", "]"}]}], "+", 
       RowBox[{"3.7", " ", 
        RowBox[{"x", "[", "6", "]"}]}]}], ",", 
      RowBox[{
       RowBox[{
        RowBox[{"-", "0.7"}], " ", 
        RowBox[{"x", "[", "1", "]"}]}], "+", 
       RowBox[{"3.4", " ", 
        RowBox[{"x", "[", "3", "]"}]}], "+", 
       RowBox[{"4.5", " ", 
        RowBox[{"x", "[", "4", "]"}]}], "-", 
       RowBox[{"2.1", " ", 
        RowBox[{"x", "[", "5", "]"}]}], "+", 
       RowBox[{"4.2", " ", 
        RowBox[{"x", "[", "6", "]"}]}]}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"3.4", " ", 
        RowBox[{"x", "[", "2", "]"}]}], "-", 
       RowBox[{"0.6", " ", 
        RowBox[{"x", "[", "3", "]"}]}], "-", 
       RowBox[{"1.2", " ", 
        RowBox[{"x", "[", "4", "]"}]}], "-", 
       RowBox[{"0.6", " ", 
        RowBox[{"x", "[", "5", "]"}]}]}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"1.6", " ", 
        RowBox[{"x", "[", "1", "]"}]}], "+", 
       RowBox[{"2.8", " ", 
        RowBox[{"x", "[", "3", "]"}]}], "+", 
       RowBox[{"3.6", " ", 
        RowBox[{"x", "[", "4", "]"}]}], "+", 
       RowBox[{"2.1", " ", 
        RowBox[{"x", "[", "5", "]"}]}], "-", 
       RowBox[{"0.7", " ", 
        RowBox[{"x", "[", "6", "]"}]}]}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"2.3", " ", 
        RowBox[{"x", "[", "1", "]"}]}], "+", 
       RowBox[{"3.2", " ", 
        RowBox[{"x", "[", "3", "]"}]}], "+", 
       RowBox[{"4.9", " ", 
        RowBox[{"x", "[", "5", "]"}]}]}]}], "}"}]}], ";"}], " ", "\n"}], "\n", 
 RowBox[{"Column", "[", "linearformulas", "]"}]}], "Input"],

Cell["\<\
Then you are given values 
             y[1], . . . ,y[6],
one for each linear formula, and you set 
             linearformula[k] = y[k]:\
\>", "Text"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Clear", "[", "y", "]"}], ";"}], " "}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"Y", "=", 
    RowBox[{"{", 
     RowBox[{"2.4", ",", 
      RowBox[{"-", "0.4"}], ",", "2.7", ",", 
      RowBox[{"-", "1.9"}], ",", "0.8", ",", "1.6"}], "}"}]}], ";"}], 
  " "}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"y", "[", "k_", "]"}], ":=", 
    RowBox[{"Y", "\[LeftDoubleBracket]", "k", "\[RightDoubleBracket]"}]}], 
   ";"}], " ", "\n"}], "\n", 
 RowBox[{"linearsystem", " ", "=", " ", "\n", 
  RowBox[{"Column", "[", 
   RowBox[{"Thread", "[", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"linearformulas", "[", 
        RowBox[{"[", "k", "]"}], "]"}], " ", "==", " ", 
       RowBox[{"y", "[", "k", "]"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"k", ",", "1", ",", 
        RowBox[{"Length", "[", "linearformulas", "]"}]}], "}"}]}], "]"}], 
    "]"}], "]"}]}]}], "Input"],

Cell[CellGroupData[{

Cell[TextData[{
 StyleBox["Once you have the coefficient matrix A for this linear system, \
then going about solving the linear system is not bad at all.\nBut reading \
off and manually typing A into ",
  Evaluatable->False,
  AspectRatioFixed->True],
 StyleBox["Mathematica",
  Evaluatable->False,
  AspectRatioFixed->True,
  FontSlant->"Italic"],
 StyleBox[" code is a real pain in the butt.\nHow can you get ",
  Evaluatable->False,
  AspectRatioFixed->True],
 StyleBox["Mathematica",
  Evaluatable->False,
  AspectRatioFixed->True,
  FontSlant->"Italic"],
 StyleBox["  pluck out that coefficient matrix for you?",
  Evaluatable->False,
  AspectRatioFixed->True]
}], "Text"],

Cell["Answer:", "Special1"],

Cell["Here's one way:", "SmallText"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"xvariables", "=", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"x", "[", "k", "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"k", ",", "1", ",", "6"}], "}"}]}], "]"}]}], ";"}], 
  " "}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"A", "=", 
    StyleBox[
     RowBox[{
      RowBox[{"Outer", "[", 
       RowBox[{"D", ",", 
        RowBox[{"{", "linearformulas", "}"}], ",", "xvariables"}], "]"}], 
      "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}],
     FontColor->RGBColor[0, 0, 1]]}], 
   StyleBox[";",
    FontColor->RGBColor[0, 0, 1]]}], 
  StyleBox[" ",
   FontColor->RGBColor[0, 0, 1]], "\n"}], "\n", 
 RowBox[{"MatrixForm", "[", "A", "]"}]}], "Input"],

Cell["Check it out:", "SmallText"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"hitdim", "=", "6"}], ";", " ", 
   RowBox[{"hangdim", "=", "6"}], ";"}], " "}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"X", "=", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"x", "[", "k", "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"k", ",", "1", ",", "hitdim"}], "}"}]}], "]"}]}], ";"}], 
  " "}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"Y", "=", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"y", "[", "k", "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"k", ",", "1", ",", "hangdim"}], "}"}]}], "]"}]}], ";"}], " ", 
  "\n"}], "\n", 
 RowBox[{"Column", "[", 
  RowBox[{"Thread", "[", 
   RowBox[{
    RowBox[{"A", ".", "X"}], " ", "==", "  ", "Y"}], "]"}], "]"}]}], "Input"],

Cell["Compare:", "SmallText"],

Cell[BoxData["linearsystem"], "Input"],

Cell["\<\
Worked like a charm. 
The solution of this linear system is:\
\>", "SmallText"],

Cell[BoxData[
 RowBox[{"X", "=", 
  RowBox[{
   RowBox[{"Inverse", "[", "A", "]"}], ".", "Y"}]}]], "Input"],

Cell["Check:", "SmallText"],

Cell[BoxData[{"Y", "\n", 
 RowBox[{"A", ".", "X"}]}], "Input"],

Cell["Had it all the way.", "SmallText"]
