     "]"}], ";"}], " "}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"A", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"a", ",", "b"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"c", ",", "d"}], "}"}]}], "}"}]}], ";"}], " "}], "\n", 
 RowBox[{"linearityproperty", "=", 
  RowBox[{
   RowBox[{"A", ".", 
    RowBox[{"(", 
     RowBox[{"r", " ", 
      RowBox[{"{", 
       RowBox[{"x", ",", "y"}], "}"}]}], ")"}]}], "==", 
   RowBox[{"Expand", "[", 
    RowBox[{"r", " ", 
