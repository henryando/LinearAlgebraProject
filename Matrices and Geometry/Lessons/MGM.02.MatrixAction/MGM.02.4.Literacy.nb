Cell["\<\
Given that the area of the unit circle measures out to \[Pi] square units, \
what does the area of the ellipse plotted above measure out to?\
\>", "Text"],

Cell["L.12)", "Subsubsection"],

Cell["Go with  this matrix A:", "Text"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"xstretch", "=", 
    RowBox[{
     FractionBox["1", "2"], " ", 
     RowBox[{"Random", "[", 
      RowBox[{"Integer", ",", 
       RowBox[{"{", 
        RowBox[{"1", ",", "5"}], "}"}]}], "]"}]}]}], ";"}], " "}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"ystretch", "=", 
    RowBox[{
     FractionBox["1", "6"], " ", 
     RowBox[{"Random", "[", 
      RowBox[{"Integer", ",", 
       RowBox[{"{", 
        RowBox[{"1", ",", "5"}], "}"}]}], "]"}]}]}], ";"}], " "}], "\n", 
 RowBox[{
  RowBox[{"Clear", "[", 
   RowBox[{
   "x", ",", "y", ",", "t", ",", "hitplotter", ",", "hitpointplotter", ",", 
    "pointcolor", ",", "matrix2D"}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"tlow", ",", "thigh"}], "}"}], "=", 
   RowBox[{"{", 
    RowBox[{"0", ",", 
     RowBox[{"2", " ", "\[Pi]"}]}], "}"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"ranger", "=", 
   RowBox[{"Max", "[", 
    RowBox[{"{", 
     RowBox[{"xstretch", ",", "ystretch", ",", "1"}], "}"}], "]"}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"x", "[", "t_", "]"}], ",", 
     RowBox[{"y", "[", "t_", "]"}]}], "}"}], "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Cos", "[", "t", "]"}], ",", 
     RowBox[{"Sin", "[", "t", "]"}]}], "}"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"pointcolor", "[", "t_", "]"}], "=", 
   RowBox[{"RGBColor", "[", 
    RowBox[{
     RowBox[{"0.5", " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"Cos", "[", "t", "]"}], "+", "1"}], ")"}]}], ",", 
     RowBox[{"0.5", " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"Sin", "[", "t", "]"}], "+", "1"}], ")"}]}], ",", "0"}], 
    "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"jump", "=", 
   FractionBox[
    RowBox[{"thigh", "-", "tlow"}], "8"]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"hitplotter", "[", "matrix2D_", "]"}], ":=", 
   RowBox[{"ParametricPlot", "[", 
    RowBox[{
     RowBox[{"matrix2D", ".", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"x", "[", "t", "]"}], ",", 
        RowBox[{"y", "[", "t", "]"}]}], "}"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", "tlow", ",", "thigh"}], "}"}], ",", 
     RowBox[{"PlotStyle", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Thickness", "[", "0.01", "]"}], ",", "Blue"}], "}"}], 
       "}"}]}], ",", 
     RowBox[{"PlotRange", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"-", "ranger"}], ",", "ranger"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"-", "ranger"}], ",", "ranger"}], "}"}]}], "}"}]}], ",", 
     RowBox[{"AxesLabel", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{"\"\<x\>\"", ",", "\"\<y\>\""}], "}"}]}]}], "]"}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"hitpointplotter", "[", "matrix2D_", "]"}], ":=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"Graphics", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"pointcolor", "[", "t", "]"}], ",", 
        RowBox[{"PointSize", "[", "0.035", "]"}], ",", 
        RowBox[{"Point", "[", 
         RowBox[{"matrix2D", ".", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"x", "[", "t", "]"}], ",", 
            RowBox[{"y", "[", "t", "]"}]}], "}"}]}], "]"}]}], "}"}], "]"}], 
     ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", "tlow", ",", 
       RowBox[{"thigh", "-", "jump"}], ",", "jump"}], "}"}]}], "]"}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{"A", "=", 
   RowBox[{"DiagonalMatrix", "[", 
    RowBox[{"{", 
     RowBox[{"xstretch", ",", "ystretch"}], "}"}], "]"}]}], ";"}], "\n", 
 RowBox[{"MatrixForm", "[", "A", "]"}]}], "Input",
 CellOpen->False,
 AspectRatioFixed->True],

