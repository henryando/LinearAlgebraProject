}], "Text"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   StyleBox[
    RowBox[{"A", " ", "=", 
     TagBox[
      RowBox[{"(", GridBox[{
         {"2.7", "1.3"},
         {"0.5", 
          RowBox[{"-", "1.8"}]}
        }], ")"}],
      MatrixForm[#]& ]}],
    FontColor->RGBColor[0, 0, 1]], ";"}], " "}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"ranger", " ", "=", 
    RowBox[{"2", " ", 
     RowBox[{"Max", "[", 
      RowBox[{
       RowBox[{"SingularValues", "[", "A", "]"}], "[", 
       RowBox[{"[", 
        StyleBox["2",
         FontColor->RGBColor[1, 0, 0]], "]"}], "]"}], "]"}]}]}], ";"}], 
  "  "}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"Clear", "[", "t", "]"}], ";"}], " "}], "\n", 
 RowBox[{"ellipseplot", " ", "=", " ", 
  RowBox[{"ParametricPlot", "[", 
   RowBox[{
    RowBox[{
     StyleBox["A",
      FontColor->RGBColor[0, 0, 1]], ".", 
