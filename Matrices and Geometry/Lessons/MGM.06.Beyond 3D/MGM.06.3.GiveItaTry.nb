 RowBox[{
  RowBox[{
   RowBox[{"Clear", "[", "z", "]"}], ";"}], " "}], "\n", 
 RowBox[{"charequation", "=", 
  RowBox[{
   RowBox[{
    SuperscriptBox["z", "4"], "+", 
    RowBox[{"2.9", " ", 
     SuperscriptBox["z", "3"]}], "+", 
    RowBox[{"3.04", " ", 
     SuperscriptBox["z", "2"]}], "+", 
    RowBox[{"1.356", " ", "z"}], "+", "0.216"}], "==", "0"}]}]}], "Input"],

