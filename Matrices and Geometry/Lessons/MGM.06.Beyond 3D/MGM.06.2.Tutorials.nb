  RowBox[{
   RowBox[{"A", "=", 
    RowBox[{"Table", "[", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"t", "[", "i", "]"}], 
       StyleBox[
        RowBox[{"j", "-", "1"}],
        FontColor->RGBColor[1, 0, 0]]], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "1", ",", "dim"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"j", ",", "1", ",", "dim"}], "}"}]}], "]"}]}], ";"}], " ", 
  "\n"}], "\n", 
 RowBox[{"MatrixForm", "[", "A", "]"}]}], "Input"],

Cell["Some folks like to call this a Vandermonde matrix.", "Special5"],

