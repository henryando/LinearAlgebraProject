Cell["\<\
Define a trig polynomial whose constituent Cosine and Sine waves are fixed to \
match the period of f[t] and has one undetermined coefficient x[k] for each \
point\
\>", "Text"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"b", "=", 
    RowBox[{
     FractionBox["1", "2"], " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"Length", "[", "fpoints", "]"}], "-", "1"}], ")"}]}]}], ";"}], 
  " ", "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"Clear", "[", 
    RowBox[{"trigpoly", ",", "x", ",", "t", ",", "k"}], "]"}], ";"}], 
  " "}], "\n", 
 RowBox[{
  RowBox[{"trigpoly", "[", "t_", "]"}], "=", 
  RowBox[{
   RowBox[{"x", "[", "1", "]"}], "+", 
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"k", "=", "2"}], 
     RowBox[{"b", "+", "1"}]], 
    RowBox[{
     RowBox[{"x", "[", "k", "]"}], " ", 
     RowBox[{"Cos", "[", 
      FractionBox[
       RowBox[{
        RowBox[{"(", 
         RowBox[{"k", "-", "1"}], ")"}], " ", 
        StyleBox[
         RowBox[{"(", 
          RowBox[{"2", " ", "\[Pi]"}], ")"}],
         FontColor->RGBColor[0, 0, 1]], 
        StyleBox[" ",
         FontColor->RGBColor[0, 0, 1]], 
        StyleBox["t",
         FontColor->RGBColor[0, 0, 1]]}], 
       StyleBox["L",
        FontColor->RGBColor[0, 0, 1]]], "]"}]}]}], "+", 
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"k", "=", " ", 
      RowBox[{"b", "+", "2"}]}], 
     RowBox[{"Length", "[", "fpoints", "]"}]], 
    RowBox[{
     RowBox[{"x", "[", "k", "]"}], " ", 
     RowBox[{"Sin", "[", 
      FractionBox[
       RowBox[{
        RowBox[{"(", 
         RowBox[{"k", "-", "b", "-", "1"}], ")"}], " ", 
        StyleBox[
         RowBox[{"(", 
          RowBox[{"2", " ", "\[Pi]"}], ")"}],
         FontColor->RGBColor[0, 0, 1]], 
        StyleBox[" ",
         FontColor->RGBColor[0, 0, 1]], 
        StyleBox["t",
         FontColor->RGBColor[0, 0, 1]]}], 
       StyleBox["L",
        FontColor->RGBColor[0, 0, 1]]], "]"}]}]}]}]}]}], "Input"],

