Cell[TextData[{
 "B is the rotation matrix whose hits rotate everything by ",
 Cell[BoxData[
  FractionBox["\[Pi]", "4"]]],
 " counterclockwise radians.\nHere's what you get when you hit the unit \
circle with A.B: "
}], "Text"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"s", " ", "=", " ", 
   RowBox[{"Pi", "/", "4"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"perpframe", "[", "1", "]"}], ",", 
     RowBox[{"perpframe", "[", "2", "]"}]}], "}"}], " ", "=", " ", "\n", 
   "           ", 
   RowBox[{"N", "[", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Cos", "[", "s", "]"}], ",", 
        RowBox[{"Sin", "[", "s", "]"}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Cos", "[", 
         RowBox[{"s", " ", "+", " ", 
          RowBox[{"Pi", "/", "2"}]}], "]"}], ",", 
        RowBox[{"Sin", "[", 
         RowBox[{"s", " ", "+", " ", 
          RowBox[{"Pi", "/", "2"}]}], "]"}]}], "}"}]}], "}"}], "]"}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"a", ",", "c"}], "}"}], " ", "=", " ", 
   RowBox[{"perpframe", "[", "1", "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"b", ",", "d"}], "}"}], " ", "=", " ", 
   RowBox[{"perpframe", "[", "2", "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"B", " ", "=", " ", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"a", ",", "b"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"c", ",", "d"}], "}"}]}], "}"}]}], ";"}], "\n"}], "\n", 
 RowBox[{" ", 
  RowBox[{"Show", "[", 
   RowBox[{
    RowBox[{"hitplotter", "[", 
     RowBox[{"A", ".", "B"}], "]"}], ",", 
    RowBox[{"PlotLabel", "->", "\"\<After the hit with A.B\>\""}]}], 
   "]"}]}]}], "Input",
 CellOpen->False],

