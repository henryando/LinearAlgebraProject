       FractionBox["t", "2"], "]"}]}], "-", 
     RowBox[{"4", " ", 
      RowBox[{"Abs", "[", 
       RowBox[{
        RowBox[{"0.5", " ", "t"}], "-", 
        RowBox[{"Round", "[", 
         RowBox[{"0.5", " ", "t"}], "]"}]}], "]"}]}]}]}], ";"}], " "}], "\n", 

 RowBox[{
  RowBox[{
   RowBox[{"L", "=", "2"}], ";"}], " "}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"cycles", "=", "3"}], ";"}], " ", "\n"}], "\n", 
 RowBox[{
  RowBox[{"Plot", "[", 
   RowBox[{
    RowBox[{"f", "[", "t", "]"}], ",", 
    RowBox[{"{", 
